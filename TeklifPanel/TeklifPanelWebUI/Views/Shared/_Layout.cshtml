<!DOCTYPE html>
<html lang="tr">

<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="Abstract" content="" />
    <meta name="Author" content="" />
    <title>@TempData["Title"]</title>
    <!--Kelimeler Max. 5 Kelime -->
    <script src="~/js/checkInput.js"></script>
    <meta name="keywords" content="" />

    <!-- Sayfa Açıklaması Max. 115 Karakter -->
    <meta name="description" content="" />
    <meta property="og:description" content="" />
    <meta itemprop="description" content="" />
    <meta name="twitter:description" content="" />
    @await Html.PartialAsync("_Links")

</head>

<body>
    <div class="wrapper">
        <!-- Sidebar  -->
        @*<partial name="_SolMenu" />*@
        @await Component.InvokeAsync("LeftMenu")
        <div id="content">
            <!-- Page Content  -->
            @*<partial name="_NavBar" />*@
            @await Component.InvokeAsync("NavBar")

            @RenderBody()
        </div>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
        </div>
    </footer>

    @await Html.PartialAsync("_Scripts")
    @await RenderSectionAsync("Scripts", required: false)
    <script>
        var divs = document.querySelectorAll(".custom-popover");
        console.log(divs);
        divs.forEach(function (div) {
            div.addEventListener("click", function () {
                console.log(div);
                divs.forEach(function (d) {

                    d.classList.add("d-none");
                });
                this.classList.remove("d-none");
            });
        });
    </script>

   


</body>

</html>

